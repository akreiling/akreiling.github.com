---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
    <title>Voodoo Griller</title>
    <link href="http://www.voodoogriller.com/feed/" rel="self"/>
    <link href="http://www.voodoogriller.com/"/>
    <updated>{{ site.time | date_to_xmlschema }}</updated>
    <id>http://www.voodoogriller.com/</id>
    <author>
        <name>Andrew Kreiling</name>
        <email>akreiling@pobox.com</email>
    </author>

    {% for post in site.posts %}
    <entry>
        <title>{{ post.title }}</title>
        <link href="http://www.voodoogriller.com{{ post.url }}"/>
        <updated>{{ post.date | date_to_xmlschema }}</updated>
        <id>http://www.voodoogriller.com{{ post.id }}</id>
        <content type="html">{{ post.content | xml_escape }}</content>
    </entry>
    {% endfor %}
</feed>
